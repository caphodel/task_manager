<thead>
	<tr>
		{{#each columns}}
		<th class="j-border-bot2 j-bg1 j-border-right1"><div class="j-wrap-hide j-pad1 j-border-box">{{{this}}}<div class="j-table-sort j-table-left1 j-bg1"></div></div></th>
		{{/each}}
	</tr>
	<tr>
		{{#each columns}}
		<th class="j-border-right1"><div class="j-hide-size j-pad3 j-border-box">{{{this}}}<j-resize drag="true" direction="horizontal" style="position: absolute; bottom: 0;"></j-resize></div></th>
		{{/each}}
	</tr>
</thead>
