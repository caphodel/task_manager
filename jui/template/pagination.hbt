<j-toolbar class="j-table-pagination" style="align-items: baseline;">
    <j-button class="j-table-first"><i class="fa fa-fast-backward"></i></j-button>
    <j-button class="j-table-prev"><i class="fa fa-backward"></i></j-button>
    <j-textfield class="j-table-page" no-label="true" style="width: 60px;"></j-textfield>
    <j-button class="j-table-next"><i class="fa fa-forward"></i></j-button>
    <j-button class="j-table-last"><i class="fa fa-fast-forward"></i></j-button>
    <j-button class="j-table-refresh"><i class="fa fa-repeat"></i></j-button>
    <j-spacer></j-spacer><span class="j-table-data-info"></span>
</j-toolbar>
